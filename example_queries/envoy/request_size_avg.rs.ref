pub fn get_value_for_storage(
    target_graph: &Graph<
        (
            std::string::String,
            IndexMap<std::string::String, std::string::String>,
        ),
        (),
    >,
    mapping: &Vec<(NodeIndex, NodeIndex)>,
    stored_data: &FerriedData,
) -> Option<String> {
    let value: String;
    let node_ptr = get_node_with_id(target_graph, "a.request.total_size".to_string());
        if node_ptr.is_none() {
           log::error!("Node a.request.total_size not found");
                return None;
        }
        let mut trace_node_idx_opt = None;
        for map in mapping {
            if target_graph.node_weight(map.0).unwrap().0 == "a.request.total_size" {
                trace_node_idx_opt = Some(map.1);
                break;
            }
        }
        if trace_node_idx_opt.is_none() {
            log::error!("Node index a.request.total_size not found.");
            // we have not yet collected the return property or have a mapping error
            return None;
        }
        let trace_node_idx = trace_node_idx_opt.unwrap();
        if !&stored_data
            .trace_graph
            .node_weight(trace_node_idx)
            .unwrap()
            .1
            .contains_key("avg(a.request.total_size)")
        {
            // we have not yet collected the return property
            log::error!("Missing return property avg(a.request.total_size)");
            return None;
        }
        let ret = &stored_data.trace_graph.node_weight(trace_node_idx).unwrap().1[ "avg(a.request.total_size)" ];

        value = ret.to_string();


    return Some(value);
}


